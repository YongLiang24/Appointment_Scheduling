<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.LoginFXMLController">
   <children>
      <HBox alignment="CENTER" layoutX="1.0" prefHeight="66.0" prefWidth="600.0">
         <children>
            <Label text="Appointment System INC">
            </Label>
         </children></HBox>
      <VBox alignment="TOP_CENTER" layoutY="66.0" prefHeight="333.0" prefWidth="600.0">
         <children>
            <HBox alignment="BOTTOM_CENTER" prefHeight="57.0" prefWidth="600.0">
               <children>
                  <Label text="Username:">
                     <HBox.margin>
                       
                     </HBox.margin></Label>
                  <TextField fx:id="userNameTxt" maxWidth="149.0" prefWidth="149.0" text="dwad" />
               </children>
            </HBox>
            <HBox alignment="CENTER" layoutX="10.0" layoutY="10.0" prefHeight="57.0" prefWidth="600.0">
               <children>
                  <Label text="Password:">
                     <HBox.margin>
                       
                     </HBox.margin>
                  </Label>
                  <TextField maxWidth="150.0" prefHeight="25.0" prefWidth="150.0" />
               </children>
            </HBox>
            <Button defaultButton="true" mnemonicParsing="false" prefHeight="29.0" prefWidth="105.0" text="Login">
               <font>
                  <Font size="13.0" />
               </font>
            </Button>
            <HBox alignment="CENTER" prefHeight="85.0" prefWidth="600.0">
               <children>
                  <Label text="Current Location:">
                     <padding>
                        <Insets right="10.0" />
                     </padding>
                     <font>
                        <Font name="System Bold" size="12.0" />
                     </font>
                  </Label>
                  <Label fx:id="UserLocation" text="placeholder">
                     <font>
                        <Font size="13.0" />
                     </font>
                  </Label>
               </children>
            </HBox>
            <Button defaultButton="true" mnemonicParsing="false" onAction="#exitApp" prefHeight="25.0" prefWidth="105.0" text="Quit" />
         </children>
      </VBox>
   </children>
</AnchorPane>
