<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.LoginFXMLController">
   <children>
      <HBox alignment="CENTER" layoutX="1.0" prefHeight="66.0" prefWidth="600.0">
         <children>
            <Label text="Appointment System INC">
            </Label>
         </children></HBox>
      <VBox alignment="TOP_CENTER" layoutY="66.0" prefHeight="333.0" prefWidth="600.0">
         <children>
            <HBox alignment="BOTTOM_CENTER" prefHeight="57.0" prefWidth="600.0">
               <children>
                  <Label fx:id="usernameLabel" text="Username:">
                     <HBox.margin>
                       
                     </HBox.margin></Label>
                  <TextField fx:id="UsernameTxt" maxWidth="149.0" prefWidth="149.0" />
               </children>
            </HBox>
            <HBox alignment="CENTER" layoutX="10.0" layoutY="10.0" prefHeight="57.0" prefWidth="600.0">
               <children>
                  <Label fx:id="PasswordLabel" text="Password:">
                     <HBox.margin>
                       
                     </HBox.margin>
                  </Label>
                  <TextField fx:id="PasswordTxt" maxWidth="150.0" prefHeight="25.0" prefWidth="150.0" />
               </children>
            </HBox>
            <Button fx:id="LoginBtnLabel" defaultButton="true" mnemonicParsing="false" onAction="#loginHandler" prefHeight="29.0" prefWidth="105.0" text="Login" />
            <HBox alignment="CENTER" prefHeight="85.0" prefWidth="506.0">
               <children>
                  <Label fx:id="locationLabel" text="Current Location" />
                  <Label text=":  " />
                  <Label fx:id="UserLocation" text="placeholder" />
               </children>
            </HBox>
            <Button fx:id="quitLabel" defaultButton="true" mnemonicParsing="false" onAction="#exitApp" prefHeight="25.0" prefWidth="105.0" text="Quit" />
            <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0">
               <children>
                  <Label fx:id="ErrorMsg" />
               </children>
            </HBox>
         </children>
      </VBox>
   </children>
</AnchorPane>
