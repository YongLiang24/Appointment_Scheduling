<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.AddCustomerController">
   <children>
      <ComboBox fx:id="Country_Combo" layoutX="149.0" layoutY="53.0" onAction="#selectCountry" prefWidth="150.0" promptText="Country Select" />
      <ComboBox fx:id="Division_Combo" disable="true" layoutX="300.0" layoutY="53.0" onAction="#selectDivision" prefWidth="150.0" promptText="Division Select" />
      <Button layoutX="226.0" layoutY="286.0" mnemonicParsing="false" onAction="#createCustomerBtn" prefWidth="59.42" text="Create" />
      <Label layoutX="251.0" layoutY="14.0" text="Customer Creation" />
      <Button layoutX="301.0" layoutY="286.0" mnemonicParsing="false" onAction="#returnBtn" text="Go Back" />
      <Label fx:id="WarningMsg" layoutX="150.0" layoutY="324.0" text="Message:" />
      <TextField fx:id="CustomerName" layoutX="226.0" layoutY="100.0" />
      <TextField fx:id="Address" layoutX="226.0" layoutY="140.0" />
      <TextField fx:id="PostalCode" layoutX="226.0" layoutY="175.0" />
      <TextField fx:id="PhoneNumber" layoutX="226.0" layoutY="212.0" />
      <Label layoutX="130.0" layoutY="104.0" text="Customer Name:" />
      <Label layoutX="170.0" layoutY="144.0" text="Address:" />
      <Label layoutX="149.0" layoutY="179.0" text="Postal Code:" />
      <Label layoutX="133.0" layoutY="216.0" text="Phone Number:" />
   </children>
</AnchorPane>
